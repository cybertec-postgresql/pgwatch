          "measurement": "pgbouncer_stats",
          "metricColumn": "none",
          "orderByTime": "ASC",
          "policy": "default",
          "rawQuery": true,
          "rawSql": "select\n  $__timeGroup(time, $__interval),\n  avg((c - c_lag) / extract(epoch from time - time_lag)) as \"TPS\"\nfrom (\n        select \n          sum((data->>'total_xact_count')::int8) as c, lag(sum((data->>'total_xact_count')::int8)) over w as c_lag,\n          date_trunc('minute', time) as time, lag(date_trunc('minute', time)) over w as time_lag\n        from pgbouncer_stats\n        where dbname = '$dbname' and $__timeFilter(time)\n        group by date_trunc('minute', time)\n        window w as (order by date_trunc('minute', time))        \n) x\nwhere c >= c_lag and time > time_lag\ngroup by 1\norder by 1",
          "refId": "txs_per_second",
          "resultFormat": "time_series",
          "select": [
