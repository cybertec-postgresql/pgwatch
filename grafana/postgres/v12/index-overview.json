          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT DISTINCT ON (dbname, \"Table Name\", \"Recommendation\")\n    dbname AS \"Source Name\",\n    tag_data->>'object_name' AS \"Table Name\",\n    data->>'affected_query_count' AS \"Affected Queries\",\n    data->>'query_id' AS \"Heaviest Query\",\n    data->>'recommendation' AS \"Recommendation\"\nFROM\n(\n  SELECT\n    *,\n    ROW_NUMBER() OVER(PARTITION BY dbname ORDER BY data->>'epoch_ns' DESC) AS rn\n  FROM recommendations\n  WHERE $__timeFilter(time) AND tag_data->>'reco_topic' = 'create_index'\n)\nWHERE \n  rn <= 10\n  AND dbname IN ($dbname)\nORDER BY dbname, \"Table Name\", \"Recommendation\"",
